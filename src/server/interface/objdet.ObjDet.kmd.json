{
    "remoteClasses": [
        {
            "name": "ObjDet",
            "extends": "OpenCVFilter",
            "doc": "ObjDet interface. Documentation about the module.",
            "constructor": {
                "doc": "Create an element.",
                "params": [
                    {
                        "name": "mediaPipeline",
                        "doc": "the parent :rom:cls:`MediaPipeline`.",
                        "type": "MediaPipeline",
                        "final": true
                    }
                ]
            },
            "methods": [
                {
                    "name": "setConfidence",
                    "doc": "Set the confidence threshold for objects",
                    "params": [
                        {
                            "name": "confidence",
                            "doc": "0.01~0.99",
                            "type": "float"
                        }
                    ]
                },
                {
                    "name": "setBoxLimit",
                    "doc": "Set max box number to return and draw",
                    "params": [
                        {
                            "name": "boxLimit",
                            "doc": "1~100",
                            "type": "int"
                        }
                    ]
                },
                {
                    "name": "setIsDraw",
                    "doc": "Set whether to draw box or not",
                    "params": [
                        {
                            "name": "isDraw",
                            "doc": "true/false",
                            "type": "boolean"
                        }
                    ]
                },
                {
                    "name": "heartbeat",
                    "doc": "Heartbeat for keeping model active",
                    "params": [
                        {
                            "name": "sessionId",
                            "doc": "",
                            "type": "String"
                        }
                    ]
                },
                {
                    "name": "initSession",
                    "doc": "",
                    "params": []
                },
                {
                    "name": "startInferring",
                    "doc": "Start detection",
                    "params": []
                },
                {
                    "name": "stopInferring",
                    "doc": "Stop detection",
                    "params": []
                },
                {
                    "name": "destroy",
                    "doc": "Explicitly destroy the model (return to model pool)",
                    "params": []
                }
            ],
            "events": ["boxDetected", "sessionInitState", "paramSetState"]
        }
    ],
    "events": [
        {
            "name": "boxDetected",
            "doc": "return detected objects",
            "extends": "Media",
            "properties": [
                {
                    "name": "objectJSON",
                    "doc": "JSON format, {x1:0,y1:0,x2:0,y2:0,name:,confi:0}",
                    "type": "String"
                }
            ]
        },
        {
            "name": "sessionInitState",
            "doc": "return the session init state",
            "extends": "Media",
            "properties": [
                {
                    "name": "stateJSON",
                    "doc": "JSON format, {state:,msg:,sessionId:}",
                    "type": "String"
                }
            ]
        },
        {
            "name": "paramSetState",
            "doc": "return param set state",
            "extends": "Media",
            "properties": [
                {
                    "name": "stateJSON",
                    "doc": "JSON format, {state:,param_name:}",
                    "type": "String"
                }
            ]
        }
    ]
}
